{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/core/renderer/src/env.d.ts",
    "src/core/renderer/src/**/*",
    "src/core/renderer/src/**/*.tsx",
    "src/core/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/core/renderer/src/*"
      ],
      "@preload/*": [
        "src/core/preload/*"
      ],
      "@main/*": [
        "src/core/main/*"
      ],


    }
  }
}
